{
  "name": "field2fridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:code": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,cts,mts,json,css,scss,mdx,html}\"",
    "lint:all": "pnpm lint:fix && pnpm lint && pnpm format:code",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "build:contract": "cd blockchain/bnb/hardhat && pnpm run compile",
    "convex:dev": "node ./scripts/convex-dev.cjs",
    "convex:deploy": "npx convex deploy -y",
    "convex:redeploy": "pnpm convex:deploy && pnpm convex:dev",
    "convex:reset": "dotenv -e .env -e .env.local -- tsx scripts/reset-convex.ts",
    "payments:dev": "tsx payments/server.ts",
    "contracts:compile": "cd blockchain/bnb/hardhat && pnpm install && pnpm run compile"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@tanstack/react-query": "^5.90.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.30.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "next": "^15.0.0",
    "next-themes": "^0.4.6",
    "pino-pretty": "^13.1.3",
    "q402": "github:quackai-labs/Q402",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.41.2",
    "wagmi": "^2.19.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^4.0.15",
    "ws": "^8.18.0"
  }
}
